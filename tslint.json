{
  "extends": ["tslint:latest", "tslint-react", "tslint-microsoft-contrib"],
  "exclude": [
      "**/node_modules/**"
  ],
  "defaultSeverity": "warning",
  "rules": {
      "align": [true,
          "parameters",
          "statements"],
      "ban": false,
      "class-name": true,
      "comment-format": [true,
          "check-space"
      ],
      "curly": true,
      "eofline": true,
      "forin": true,
      "indent": [true, "spaces", 2],
      "interface-name": false,
      "jsdoc-require": [
        false
      ],
      "no-import-side-effect": false,
      "type-literal-delimiter": false,
      "no-implicit-dependencies": false,
      "socialCompetences": false,
      "react-tsx-curly-spacing": false,
      "insecure-random": false,
      "missing-jsdoc": false,
      "jsdoc-format": true,
      "prefer-method-signature": false,
      "no-reserved-keywords": false,
      "label-position": true,
      "label-undefined": true,
      "max-line-length": [true, 180],
      "no-irregular-whitespace": false,
      "member-access": false,
      "no-namespace": false,
      "ordered-imports": false,
      "no-relative-imports": false,
      "no-default-export": false,
      "export-name": false,
      "no-multiline-string": false,
      "no-http-string": false,
      "prefer-type-cast": false,
      "react-this-binding-issue": false,
      "react-unused-props-and-state": false,
      "member-ordering": [false],
      "no-any": false,
      "no-arg": true,
      "no-bitwise": false,
      "react-a11y-role-has-required-aria-props": false,
      "no-conditional-assignment": true,
      "react-a11y-event-has-role": false,
      "no-backbone-get-set-outside-model": false,
      "no-delete-expression": false,
      "jsx-wrap-multiline": false,
      "max-func-body-length": false,
      "import-name": false,
      "no-console": [true,
          "debug",
          "info",
          "time",
          "timeEnd",
          "trace"
      ],
      "jsx-no-lambda": false,
      "no-construct": true,
      "no-constructor-vars": true,
      "no-debugger": true,
      "no-duplicate-key": true,
      "no-shadowed-variable": true,
      "no-duplicate-variable": true,
      "no-duplicate-imports": false,
      "no-empty": true,
      "no-empty-interface": false,
      "no-inferrable-types": false,
      "no-internal-module": true,
      "no-require-imports": false,
      "no-string-literal": false,
      "no-switch-case-fall-through": true,
      "no-trailing-whitespace": false,
      "no-unreachable": true,
      "no-unused-expression": true,
      "no-unused-variable": true,
      "no-use-before-declare": true,
      "no-var-keyword": true,
      "no-var-requires": false,
      "object-literal-sort-keys": false,
      "newline-before-return": false,
      "one-line": [true,
          "check-open-brace",
          "check-catch",
          "check-whitespace"
      ],
      "quotemark": [true, "single", "avoid-escape"],
      "radix": true,
      "semicolon": [true, "never"],
      "switch-default": true,
      "trailing-comma": [false],
      "triple-equals": [true, "allow-null-check"],
      "typedef": [true,
          "parameter",
          "property-declaration",
          "member-variable-declaration"
      ],
      "typedef-whitespace": [true, {
          "call-signature": "nospace",
          "index-signature": "nospace",
          "parameter": "nospace",
          "property-declaration": "nospace",
          "variable-declaration": "nospace"
      }],
      "use-strict": false,
      "variable-name": false,
      "whitespace": [true,
          "check-branch",
          "check-decl",
          "check-operator",
          "check-separator",
          "check-type"
      ],
      "jsx-no-multiline-js": false,
      "no-submodule-imports": false,
      "jsx-alignment": false
  }
}

